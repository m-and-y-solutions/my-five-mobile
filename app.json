{
  "expo": {
    "name": "my-five",
    "slug": "reservmatch-mobile",
    "scheme": "myfive",
    "version": "1.1.0",
    "icon": "./assets/my-five-icon-green.png",
    "splash": {
      "image": "./assets/my-five-splash-green.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.mysolution.myfive",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/my-five-icon-green.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "allowBackup": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "myfive"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "be32c2b9-9554-4ceb-a2e0-6e15ca5e9e33"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "owner": "my-five",
    "ios": {
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Nous avons besoin d’accéder à votre photothèque pour vous permettre de sélectionner une image de profil que vous pouvez utiliser dans votre compte utilisateur."
      }
    }
  }
}
